
package productos;

/**
 *
 * @author X.V.I
 */
public class PortatilMac extends Portatil{

    public PortatilMac(String procesador, int ram, int almacenamiento, String board, String bateria, String gpu) {
        super(procesador, ram, almacenamiento, board, bateria, gpu);
    }

    @Override
    public void mostrarCaracteristicas() {
        System.out.println("Portatil Macbook\nProcesador: "+ procesador +"\nMemoria ram: " + ram +"\nAlmacenamiento:  "+ almacenamiento+ "\nBoard: "+ board+
                "\nFuente de poder: "+ bateria+ "\nGPU:" + gpu + "\n");
    }
    
}
