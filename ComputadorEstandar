package productos;

/**
 *
 * @author X.V.I
 */
public class ComputadorEstandar extends Computador{

    public ComputadorEstandar(String procesador, int ram, int almacenamiento, String board, String fuentePoder, String gpu) {
        super(procesador, ram, almacenamiento, board, fuentePoder, gpu);
    }

    @Override
    public void mostrarCaracteristicas() {
        System.out.println("Computador de Mesa\nProcesador: "+ procesador +"\nMemoria ram: " + ram +"\nAlmacenamiento:  "+ almacenamiento+ "\nBoard: "+ board+
                "\nFuente de poder: "+ fuentePoder+ "\nGPU:" + gpu + "\n"); 
    }
    
}
